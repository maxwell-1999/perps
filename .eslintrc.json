{
  "extends": ["next", "prettier", "plugin:storybook/recommended", "plugin:jsx-a11y/recommended"],
  "rules": {
    "react/no-unescaped-entities": [0],
    "@next/next/no-img-element": [0],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "plugins": ["jsx-a11y"]
}
