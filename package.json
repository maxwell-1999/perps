{
  "name": "perennial-interface-v2",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "yarn gen:abi-typings && next dev",
    "build": "yarn gen:abi-typings && next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "gen:theme-typings": "chakra-cli tokens src/components/design-system/theme/index.ts",
    "prepush": " yarn tsc && yarn test && yarn format && yarn lint && yarn localize",
    "localize:extract": "formatjs extract 'src/{pages,components}/**/*.{ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/locales/en.json",
    "localize:compile": "formatjs compile-folder --ast --format simple lang/locales lang/compiled-locales",
    "localize": "yarn localize:extract && yarn localize:compile",
    "gen:abi-typings": "typechain --target ethers-v6 --out-dir types/generated abi/*.json"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.5",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@rainbow-me/rainbowkit": "^1",
    "@tanstack/react-query": "^4.29.3",
    "@tanstack/react-query-devtools": "^4.29.7",
    "@typechain/ethers-v6": "^0.3.2",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "connectkit": "^1.2.3",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "ethers": "^6.3.0",
    "framer-motion": "^10.12.4",
    "next": "13.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^6.4.2",
    "react-table": "^7.8.0",
    "typechain": "^8.1.1",
    "typescript": "5.0.4",
    "viem": "^0.3.30",
    "wagmi": "^1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@chakra-ui/cli": "2.3.0",
    "@chakra-ui/storybook-addon": "4.0.16",
    "@formatjs/cli": "^6.1.1",
    "@formatjs/swc-plugin-experimental": "^0.3.6",
    "@next/bundle-analyzer": "^13.3.1",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/nextjs": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/prettier": "^2.7.2",
    "@types/react-table": "^7",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-formatjs": "^4.10.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-storybook": "^0.6.11",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "storybook": "^7.0.7"
  }
}
